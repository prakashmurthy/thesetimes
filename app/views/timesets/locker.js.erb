if (<%= @success %>) {
  var handler = $('#lock, #unlock');
  var handlerId = handler.attr('id');
  
  var selector = $('#selector');
  
  if (handlerId == "lock") {
    selector.attr("data-locked", "true");
    handler.attr('id', "unlock").text("Unlock");
  }
  
  if (handlerId == "unlock") {
    selector.attr("data-locked", "false");
    handler.attr("id", "lock").text("Lock");
  }
}